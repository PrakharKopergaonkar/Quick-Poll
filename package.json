  "bugs": {
    "url": "https://github.com/PrakharKopergaonkar/Quick-Poll/issues"
  },
  "homepage": "https://github.com/PrakharKopergaonkar/Quick-Poll#readme",
  "dependencies": {
    "concurrently": "^7.6.0"
  }
}
